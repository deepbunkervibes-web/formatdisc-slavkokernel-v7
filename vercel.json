{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/simulator",
      "destination": "/simulator/index.html"
    },
    {
      "source": "/simulator/",
      "destination": "/simulator/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(simulator/.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Slavko-Engine",
          "value": "V1-CANONICAL"
        }
      ]
    }
  ]
}